@model dynamic

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>Add To Campaign</title>

    <!-- Le styles -->
    <link href="/Scripts/bootstrap/bootstrap.css" rel="stylesheet" />
    <link href="/Scripts/bootstrap/bootstrap-responsive.css" rel="stylesheet" />
    <script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"></script>
</head>
<body>
    <img src="/Images/eloqua-logo.png" alt="Eloqua Concierge" width="250px" />
    <h1>Add to Campaign</h1>
    <div>Adds a contact to a Campaign in Eloqua &nbsp;</div>
    <div class="bs-docs-example">
        <fieldset>
            <select id="MovieType"  name="MovieType">

                <option value=""></option>

                <option value="0">Lead Nurturing</option>

                <option value="1">Eloqua Experience</option>

                <option selected="selected" value="2">Lead Engagement</option>

                <option value="3">Science Fiction</option>

            </select>

            <p>
                <input type="submit" class="btn-large" value="Submit" />
            </p>

        </fieldset>
    </div>
    <script type="text/javascript" src="/Scripts/bootstrap/bootstrap.js"></script>
</body>
</html>
